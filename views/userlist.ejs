<%- include header %>

<div class="jumbotron">
    <h1>UserList</h1>
</div>
<table class="table">
    <thead>
        <tr>
            <th>Username</th>
            <th>Password</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
    <% users.forEach(function(user){ %>
    <tr>
        <td><%= user.username %></td>
        <td><%= user.password %></td>
        <td>
            <a href="/users/edit/<%= user._id %>">Edit</a>
            /
            <a href="/users/delete/<%= user._id %>">Delete</a>
        </td>
    </tr>
    <% }) %>
    </tbody>
</table>
<%- include footer %>